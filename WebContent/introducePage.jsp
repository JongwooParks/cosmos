<script>
    $button = $('.css-1ryjcci');
    $detail = $('.css-22b2qv');
    $click = $('.css-1jlqepv');




    $click.click(function () {
        var $check = $(this).closest(".css-zsoya5").find(".css-22b2qv")
        if ($check.css('display') == 'none') {
            $(this).find('.css-1ryjcci').css('transform', "rotate(180deg)")
            $check.css('display', 'flex');
        } else if ($check.css('display') == 'flex') {
            $(this).find('.css-1ryjcci').css('transform', "rotate(0deg)")
            $check.css('display', 'none');

        }
    })



    var swiper = new Swiper(".mySwiper", {
        pagination: {
            el: ".swiper-pagination",
            clickable: true,
            renderBullet: function (index, className) {
                return '<span class="' + className + '">' + (index + 1) + "</span>";
            },
        },
    });

    $buttons = $('.swiper-pagination-bullet');
    console.log($buttons)




</script>
<script>
    var $categoryItem = $('.introduce_category li');
	var $returnBackPage = $('.page4_ptag');
    console.log($buttons)
    $categoryItem.click(function () {
        var num = Number($(this).attr('id'));
        console.log(num)
        if ($(this).hasClass('inactive')) {
            $buttons.eq(num - 1).trigger("click");
            $categoryItem.each((i, item) => {
                if ($categoryItem.eq(i).hasClass('active')) {
                    $categoryItem.eq(i).removeClass('active').addClass('inactive')
                }
            })
            $(this).toggleClass('inactive').toggleClass('active')
        }
    })

	$returnBackPage.click(function(){
		location.href="index.jsp";
	})



</script>
<script>
    window.addEventListener("wheel", function (e) {
        e.preventDefault();
    }, { passive: false });
    var $html = $("html");

    var page = 1;

    var lastPage = $(".content").length;

    $html.animate({ scrollTop: 0 }, 10);
    $(window).on("wheel", function (e) {

        if ($html.is(":animated")) return;

        if (e.originalEvent.deltaY > 0) {
            if (page == lastPage) return;

            page++;
        } else if (e.originalEvent.deltaY < 0) {
            if (page == 1) return;

            page--;
        }
        var posTop = (page - 1) * $(window).height();

        $html.animate({ scrollTop: posTop });
    })
    
    
    
    
</script>

